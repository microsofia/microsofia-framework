   <application name="agent1" type="fwk.agent">
      	<clusters>
   			<cluster name="registry">
				<members>
					<member host="localhost" port="8992"/>
					<member host="localhost" port="8991"/>
					<member host="localhost" port="8990"/>
				</members>
			</cluster>
		</clusters>
		<agent>
			<queue>dosample</queue>
			<implementation>do.master</implementation>
			<lookup>
				<multiplicity>one</multiplicity>
			</lookup>
		</agent>
		<databases>
			<database name="master">
				<jdbcUrl>jdbc:derby:target/master;create=true</jdbcUrl>
				<driverClassName>org.apache.derby.jdbc.EmbeddedDriver</driverClassName>
			</database>
		</databases>
		<persistence>
			<persistence-unit name="master" databasename="master">
				<properties>
					<property name="hibernate.hbm2ddl.auto" value="create"/>
				</properties>				
			</persistence-unit>
		</persistence>
		<endpoints>
			<endpoint.msofiarmi name="fwk">
